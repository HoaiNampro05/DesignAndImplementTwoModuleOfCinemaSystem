<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>KhachHangServlet451</servlet-name>
        <servlet-class>controller.KhachHangServlet451</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DangNhapServlet451</servlet-name>
        <servlet-class>controller.DangNhapServlet451</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhongChieuServlet451</servlet-name>
        <servlet-class>controller.PhongChieuServlet451</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhimServlet451</servlet-name>
        <servlet-class>controller.PhimServlet451</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LichChieuServlet451</servlet-name>
        <servlet-class>controller.LichChieuServlet451</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KhachHangServlet451</servlet-name>
        <url-pattern>/khachhang451</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DangNhapServlet451</servlet-name>
        <url-pattern>/dangnhap451</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhongChieuServlet451</servlet-name>
        <url-pattern>/phongchieu451</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhimServlet451</servlet-name>
        <url-pattern>/phim451</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LichChieuServlet451</servlet-name>
        <url-pattern>/lichchieu451</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
